{
  "name": "auction-bidding",
  "description": "Real-time auction system with proxy bidding, auto-extend, and bid history",
  "version": "1.0.0",
  "category": "marketplace",
  "dependencies": ["user-listings", "supabase-auth"],
  "files": [
    {
      "source": "lib/auction-types.ts",
      "destination": "lib/marketplace/auction-types.ts"
    },
    {
      "source": "lib/auction-engine.ts",
      "destination": "lib/marketplace/auction-engine.ts"
    },
    {
      "source": "hooks/useBidding.ts",
      "destination": "hooks/useBidding.ts"
    },
    {
      "source": "hooks/useAuctionTimer.ts",
      "destination": "hooks/useAuctionTimer.ts"
    },
    {
      "source": "components/BidForm.tsx",
      "destination": "components/marketplace/BidForm.tsx"
    },
    {
      "source": "components/BidHistory.tsx",
      "destination": "components/marketplace/BidHistory.tsx"
    },
    {
      "source": "components/AuctionTimer.tsx",
      "destination": "components/marketplace/AuctionTimer.tsx"
    },
    {
      "source": "components/CurrentBidDisplay.tsx",
      "destination": "components/marketplace/CurrentBidDisplay.tsx"
    },
    {
      "source": "components/BuyItNowButton.tsx",
      "destination": "components/marketplace/BuyItNowButton.tsx"
    },
    {
      "source": "components/OutbidAlert.tsx",
      "destination": "components/marketplace/OutbidAlert.tsx"
    },
    {
      "source": "app/api/bids/route.ts",
      "destination": "app/api/bids/route.ts"
    },
    {
      "source": "app/api/bids/[listingId]/route.ts",
      "destination": "app/api/bids/[listingId]/route.ts"
    }
  ],
  "packages": {
    "lucide-react": "^0.400.0"
  },
  "envVars": []
}

