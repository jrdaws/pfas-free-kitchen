{
  "id": "testimonials-carousel",
  "name": "Testimonial Carousel",
  "category": "testimonials",
  "description": "Sliding testimonial cards",
  "version": "1.0.0",
  "props": {
    "headline": {
      "type": "string",
      "default": "What our customers say"
    },
    "testimonials": {
      "type": "array",
      "required": true,
      "aiGenerate": true,
      "minItems": 3
    },
    "autoPlay": {
      "type": "boolean",
      "default": true
    },
    "interval": {
      "type": "number",
      "default": 5000
    }
  },
  "bestFor": ["social-proof", "multiple", "dynamic"],
  "tags": ["carousel", "slider", "animated"],
  "dependencies": ["lucide-react"]
}

